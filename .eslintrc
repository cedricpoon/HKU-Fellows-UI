{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jest": true
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-native/all"
    ],
    "rules": {
        "react/prop-types": ["error", { "ignore": ["navigation"] }]
    },
    "settings": {
        "react": {
          "version": "latest",
        }
    }
}
